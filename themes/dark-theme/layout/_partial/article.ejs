    var disqus_config = function () {
    this.page.url = 'https://chetansachdev.com/';   // Replace PAGE_URL with your page's canonical URL variable
    this.page.identifier = 'chetan-sachdev.disqus.com'; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
    };

    (function() { // DON'T EDIT BELOW THIS LINE
    var d = document, s = d.createElement('script');
    s.src = 'https://chetan-sachdev.disqus.com/embed.js';
    s.setAttribute('data-timestamp', +new Date());
    (d.head || d.body).appendChild(s);
    })();
</script>
