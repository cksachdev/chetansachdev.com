<nav>
    <ul class="pager ">
        <% if (page.prev){ %>
            <li class="button-previous"><a class="" href="<%- config.root %><%- page.prev_link %>">Previous</a></li>
        <% } %>
        <% if (page.next){ %>
            <li  class="button-next"><a href="<%- config.root %><%- page.next_link %>">Next</a></li>
        <% } %>
    </ul>
</nav>